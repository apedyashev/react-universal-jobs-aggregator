{
  "name": "react-universal-saga",
  "description": "Universal React App Starter ft. Redux Saga",
  "author": "Kawi Xiao <kawixiao@gmail.com> (http://github.com/xkawi)",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/xkawi/react-universal-saga"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xkawi/react-universal-saga/issues"
  },
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel",
    "saga",
    "redux saga"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "__SERVER__": false
    },
    "moduleFileExtensions": [
      "",
      "json",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ]
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "nodemon ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-preset-env": "1.6.0",
    "compression": "1.7.0",
    "express": "4.15.3",
    "express-http-proxy": "1.0.5",
    "file-loader": "0.11.2",
    "hoist-non-react-statics": "2.2.0",
    "humps": "2.0.1",
    "invariant": "2.2.2",
    "isomorphic-fetch": "2.2.1",
    "less": "2.7.2",
    "less-loader": "4.0.5",
    "lodash": "^4.17.4",
    "normalizr": "3.2.3",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-ga": "2.2.0",
    "react-helmet": "5.1.3",
    "react-redux": "5.0.5",
    "react-router": "4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.4",
    "scroll-behavior": "0.9.3",
    "serialize-javascript": "1.3.0",
    "serve-favicon": "2.4.3",
    "url-loader": "0.5.9",
    "warning": "3.0.0",
    "webpack-isomorphic-tools": "3.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-react-display-name": "6.25.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-typecheck": "3.9.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.24.1",
    "babel-runtime": "6.23.0",
    "better-npm-run": "0.0.15",
